<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_change_info"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.rhxorhkd.android_seoulyeojido.ChangeInfo">

    <ImageView
        android:scaleType="fitXY"
        android:id="@+id/change_info_back"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        />
    <ImageView
        android:id="@+id/chage_back"
        android:src="@drawable/back"
        android:layout_width="56dp"
        android:layout_height="56dp" />
    <TextView
        android:id="@+id/text_myinfo"
        android:layout_marginTop="19dp"
        android:textColor="#000000"
        android:textAlignment="center"
        android:textSize="20sp"
        android:layout_centerHorizontal="true"
        android:text="My Info"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <ImageView
        android:layout_marginTop="106dp"
        android:layout_centerHorizontal="true"
        android:id="@+id/change_img"
        android:layout_width="60dp"
        android:layout_height="60dp" />
    <EditText
        android:singleLine="true"
        android:padding="8dp"
        android:textSize="11sp"
        android:textColor="#000000"
        android:layout_marginTop="234dp"
        android:id="@+id/change_nickname"
        android:background="@drawable/blank"
        android:layout_centerHorizontal="true"
        android:layout_width="256dp"
        android:layout_height="40dp" />
    
    <ImageView
        android:layout_marginBottom="90dp"
        android:layout_alignParentBottom="true"
        android:id="@+id/logout"
        android:background="@drawable/logout"
        android:layout_centerHorizontal="true"
        android:layout_width="256dp"
        android:layout_height="40dp" />



</RelativeLayout>
